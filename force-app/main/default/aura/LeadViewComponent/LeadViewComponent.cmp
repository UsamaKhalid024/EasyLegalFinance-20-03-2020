                                            -->
                                            <lightning:input label="{!v.fieldLabels.Date_of_Application__c}" 
                                                             type="date" 
                                                             value="{!v.leadObj.Date_of_Application__c}" 
                                                             min="{! v.calendarMin}" 
                                                             max="{! v.calendarMax}"
                                                             class="field"
                                                             aura:id="Date_of_Application__c"
                                                             onchange="{!c.onNameChange}" />                                            
                                            
                                        </section>
                                        <button onclick="{!c.closeNameBox}" class="slds-button slds-button_icon slds-cell-edit__button slds-m-left_x-small" tabindex="0" title="Done">
                                            <lightning:icon iconName="utility:check" size="xx-small" alternativeText="Done"/>
                                        </button>
                                    </aura:set>
                                </aura:if>
                            </div>
                        </div>
                    </lightning:layoutItem>
