<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Transit_Number_Format</fullName>
    <active>true</active>
    <description>Transit Numbers must contain 5 numeric digits</description>
    <errorConditionFormula>IF( REGEX(Transit__c, &quot;^[0-9]{5}$&quot;), FALSE, TRUE)</errorConditionFormula>
    <errorDisplayField>Transit__c</errorDisplayField>
    <errorMessage>Transit Numbers must contain 5 numeric digits</errorMessage>
</ValidationRule>
